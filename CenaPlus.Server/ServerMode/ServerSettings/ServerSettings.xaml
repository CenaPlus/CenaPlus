<UserControl x:Class="CenaPlus.Server.ServerMode.ServerSettings.ServerSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab Layout="List" SelectedSource="/ServerMode/ServerSettings/Circulars.xaml">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="Circulars" Source="/ServerMode/ServerSettings/Circulars.xaml" />
                <mui:Link DisplayName="Compilers" Source="/ServerMode/ServerSettings/Compilers.xaml" />
                <mui:Link DisplayName="Judgers" Source="/ServerMode/ServerSettings/Judgers.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
